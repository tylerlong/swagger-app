    "react-dom": "15.6.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logic": "^0.12.3",
    "reselect": "^3.0.1",
    "rxjs": "^5.4.3"
