{
  "name": "swagger-app",
  "version": "0.2.3",
  "private": true,
  "description": "An app to create swagger spec.",
  "main": "build/electron.bundle.js",
  "scripts": {
    "upgrade": "yarn-upgrade-all",
    "dev": "node -r babel-register node_modules/.bin/webpack-dev-server",
    "build": "node -r babel-register node_modules/.bin/webpack --progress --colors",
    "build:watch": "yarn build --watch",
    "release": "yarn build -p",
    "test": "standard && jest",
    "test:watch": "yarn test --watch",
    "prepush": "yarn test",
    "start": "electron .",
    "dist": "rm -rf dist && electron-builder build"
  },
  "repository": "git@bitbucket.org:tylerlong/swagger-app.git",
  "author": {
    "name": "Tyler Long",
    "email": "tyler4long@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-ts-node4-bluebird": "^0.1.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "electron": "^1.7.9",
    "electron-builder": "^19.37.2",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "nock": "^9.0.22",
    "prop-types": "^15.6.0",
    "react-test-renderer": "^16.0.0",
    "redux-logic-test": "^1.0.5",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.2",
    "yarn-upgrade-all": "^0.2.0"
  },
  "dependencies": {
    "antd": "^2.13.6",
    "electron-log": "^2.2.9",
    "electron-updater": "^2.13.0",
    "indefinite": "^1.0.1",
    "js-base64": "^2.3.2",
    "moment": "^2.19.1",
    "ramda": "^0.25.0",
    "re-reselect": "^1.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logic": "^0.12.3",
    "reselect": "^3.0.1",
    "rxjs": "^5.4.3"
  },
  "jest": {
    "setupFiles": [
      "./test/shim.js",
      "./test/setup.js"
    ]
  },
  "build": {
    "appId": "swagger.ringcentral.com",
    "files": [
      "build/index.html",
      "build/electron.bundle.js",
      "build/preload.bundle.js",
      "build/index.bundle.js",
      "build/index.bundle.css",
      "!node_modules${/*}"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "dmg": {
      "contents": [
        {
          "x": 128,
          "y": 160
        },
        {
          "x": 384,
          "y": 160,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "compression": "maximum"
  }
}
