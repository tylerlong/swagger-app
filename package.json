{
  "name": "swagger-app",
  "version": "0.1.0",
  "private": true,
  "description": "An app to create swagger spec.",
  "main": "index.js",
  "scripts": {
    "upgrade": "yarn upgrade --latest && yarn remove jest && yarn add --dev jest@21.0.1",
    "dev": "node -r babel-register node_modules/.bin/webpack-dev-server",
    "build": "node -r babel-register node_modules/.bin/webpack --progress --colors",
    "release": "yarn build -p",
    "test": "standard && jest --coverage",
    "test:watch": "yarn test --watch",
    "prepush": "yarn test"
  },
  "repository": "git@bitbucket.org:tylerlong/swagger-app.git",
  "author": "Tyler Long <tyler4long@gmail.com>",
  "license": "Proprietary",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.4.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "husky": "^0.14.3",
    "jest": "21.0.1",
    "nock": "^9.0.14",
    "react-test-renderer": "^15.6.1",
    "redux-logic-test": "^1.0.5",
    "standard": "^10.0.3",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1",
    "write-file-webpack-plugin": "^4.1.0"
  },
  "dependencies": {
    "antd": "^2.13.1",
    "axios": "^0.16.2",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logic": "^0.12.2",
    "rxjs": "^5.4.3"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ]
  }
