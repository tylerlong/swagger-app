  "license": "Proprietary",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.5.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^2.13.2",
    "css-loader": "^0.28.7",
    "enzyme": "2.9.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "nock": "^9.0.21",
    "prop-types": "^15.6.0",
    "react-test-renderer": "15.6.2",
    "redux-logic-test": "^1.0.5",
